{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES5", "ES6"],
    "target": "ES6",
    "module": "CommonJS",
    "esModuleInterop": false,
    "moduleResolution": "Node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  },
  "exclude": ["node_modules"]
}
